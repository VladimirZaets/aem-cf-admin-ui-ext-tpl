{
  "displayName": "Slack Demo",
  "description": "Demo Extension to showcase custom buttons within AEM Content Fragment Admin UI",
  "version": "0.0.1",
  "templateFolder": "slack-demo",
  "actionBarButtons": [
    {
      "label": "Notify Slack",
      "needsModal": true,
      "id": "notify-slack"
    }
  ],
  "headerMenuButtons": [
    {
      "label": "Configure Slack",
      "needsModal": true,
      "id": "configure-slack"
    }
  ],
  "runtimeActions": [
    {
      "name": "notify-slack"
    },
    {
      "name": "get-slack-config"
    },
    {
      "name": "set-slack-config"
    }
  ],
  "templateInputs": {
    "LOG_LEVEL": "debug",
    "SLACK_WEBHOOK": "$SLACK_WEBHOOK",
    "SLACK_CHANNEL": "$SLACK_CHANNEL"
  },
  "templateDotEnvVars": ["SLACK_WEBHOOK", "SLACK_CHANNEL"]
}
